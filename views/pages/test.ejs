                <div class="pcoded-content">
                    <div class="pcoded-inner-content">
                        <div class="main-body">

                            <div class="page-wrapper">
                                <div class="page-body">
                                    <div class="col-lg-12 col-xl-12">
                                        <div class="card">
                                            <div class="card-body">
                                                <% for(var i=0; i < quizzes.length; i++){ %>
                                                    <div class="card">
                                                        <div class="card-body">
                                                            <div class="quiz">
                                                                <div class="question">
                                                                    <%= i+1 %>. <%= quizzes[i].question %>
                                                                </div>
                                                                <div class="answers">
                                                                    <% for(var j=0; j < quizzes[i].answers.length; j++){
                                                                        %>
                                                                        <div class="form-check">
                                                                            <input class="form-check-input answer"
                                                                                type="radio" name="group<%= i %>"
                                                                                id="<%= quizzes[i].question %><%= j %>" 
                                                                                value="<%= quizzes[i].answers[j].correct %>"
                                                                                />
                                                                                
                                                                            <label
                                                                                class="form-check-label <%= quizzes[i].answers[j].correct %>"
                                                                                for="<%= quizzes[i].question %><%= j %>">
                                                                                <%= quizzes[i].answers[j].text %>
                                                                                    
                                                                            </label>
                                                                        </div>

                                                                        <% } %>
                                                                </div>
                                                                </p>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <% } %>
                                                        <br>
                                                        <div class="row check-answers">
                                                            <div class="col-sm-12">
                                                                <button type="submit"
                                                                    class="btn-lg waves-effect waves-light btn-success btn-square btn-block">CHECK
                                                                    ANSWERS</button>
                                                            </div>
                                                        </div>
                                                        <div class="row hidden retry">
                                                            <div class="col-sm-12">
                                                                <button type="submit" id="retry"
                                                                    class="btn-lg waves-effect waves-light btn-success btn-square btn-block">RETRY</button>
                                                            </div>
                                                        </div>
                                            </div>

                                            <br>
                                        </div>

                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <input type="hidden" id="set" value="<%= set.title %>" />
            <input type="hidden" id="cards" value="<%= cards %>" />
