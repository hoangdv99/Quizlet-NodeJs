    <script>
        var correct = 0;
        var result = 0
        const inputs = document.querySelectorAll('input[class="answer"]');
        $('.check-answers').on('click', function () {
            $("input[type='radio']:checked").each(function () {
                var idVal = $(this).attr("id");
                if ($("label[for='" + idVal + "']").hasClass('false')) {
                    $("label[for='" + idVal + "']").addClass('wrong');
                }
            });
            $("input[value='true']:checked").each(function () {
                correct++;
            });
            result = Math.floor((correct / '<%= quizzes.length %>')*100);
            $('#score').text(`${result}%`);
            $('.result').removeClass('hidden');
            $('.true').addClass('correct');
            $('.check-answers').addClass('hidden');
            $('.retry').removeClass('hidden');
        });
        $('.retry').on('click', function(){
            location.reload();
        })
    </script>
