            <div class="pcoded-main-container>
                <div class=" pcoded-wrapper">
                <nav class="pcoded-navbar">
                    <div class="nav-list">
                        <div class="pcoded-inner-navbar main-menu">
                            <div class="pcoded-navigation-label">STUDY</div>
                            <ul class="pcoded-item pcoded-left-item">
                                <li class=" ">
                                    <a href="#" class="waves-effect waves-dark">
                                        <span class="pcoded-micon">
                                            <i class="feather icon-copy"></i>
                                        </span>
                                        <span class="pcoded-mtext">Flashcards</span>
                                    </a>
                                </li>
                                <li class=" ">
                                    <a href="/<%= set.id %>/learn" class="waves-effect waves-dark left-nav-menu">
                                        <span class="pcoded-micon">
                                            <i class="feather icon-book"></i>
                                        </span>
                                        <span class="pcoded-mtext">Learn</span>
                                    </a>
                                </li>
                                <li class=" ">
                                    <a href="/<%= set.id %>/write" class="waves-effect waves-dark left-nav-menu">
                                        <span class="pcoded-micon">
                                            <i class="feather icon-edit"></i>
                                        </span>
                                        <span class="pcoded-mtext">Write</span>
                                    </a>
                                </li>
                                <li class=" ">
                                    <a href="/<%= set.id %>/test" class="waves-effect waves-dark left-nav-menu">
                                        <span class="pcoded-micon">
                                            <i class="fa fa-check-square"></i>
                                        </span>
                                        <span class="pcoded-mtext">Test</span>
                                    </a>
                                </li>
                            </ul>
                        </div>
                    </div>
