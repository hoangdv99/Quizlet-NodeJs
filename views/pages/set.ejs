                <div class="pcoded-content">
                    <div class="page-header card">
                        <div class="row align-items-end">
                            <div class="col-lg-10">
                                <div class="page-header-title">
                                    <i class="feather icon-file bg-c-blue"></i>
                                    <div class="d-inline">
                                        <h5>
                                            <%= set.title %>
                                        </h5>
                                    </div>
                                </div>
                            </div>

                            <div class="col-lg-2">
                                <% if(owner.username===user.username){ %>
                                    <a type="button" href="/<%= set.id %>/edit"
                                        class="btn btn-warning btn-icon waves-effect waves-light" data-toggle="tooltip"
                                        data-placement="top" title="Edit">
                                        <i class="icofont icofont-edit"></i>
                                    </a>
                                    <a type="button" href="/<%= set.id%>/delete"
                                        onclick="return confirm('Are you sure to delete this set?')"
                                        class="btn btn-danger btn-icon waves-effect waves-light" data-toggle="tooltip"
                                        data-placement="top" title="Remove">
                                        <i class="icofont icofont-trash"></i>
                                    </a>

                                    <% } %>
                            </div>
                        </div>
                    </div>
                    <div class="pcoded-inner-content">
                        <div class="main-body">
                            <div class="page-wrapper">
                                <div class="page-body">
                                    <div id="carouselExampleControls" class="carousel slide" data-ride="carousel"
                                        data-interval="false">
                                        <div class="carousel-inner">
                                            <div class="carousel-item active">
                                                <!-- <img src="../../jpg/slide1.jpg" class="d-block w-100" alt="..."> -->
                                                <div class="row col-lg-12">
                                                    <div class="col-sm-3"></div>

                                                    <!-- <div class="card mb-3"> -->
                                                    <div class="flip-card">
                                                        <div class="flip-card-inner">
                                                            <div class="card flip-card-front">
                                                                <p class="flip-card-text">
                                                                    <%= cards[0].term %>
                                                                </p>
                                                            </div>
                                                            <div class="card flip-card-back">
                                                                <p class="flip-card-text">
                                                                    <%= cards[0].definition %>
                                                                </p>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <!-- </div> -->

                                                </div>
                                            </div>
                                            <% for(var i=1; i < cards.length; i++){ %>
                                                <div class="carousel-item">
                                                    <!-- <img src="../../jpg/slide1.jpg" class="d-block w-100" alt="..."> -->
                                                    <div class="row col-lg-12">
                                                        <div class="col-sm-3"></div>

                                                        <!-- <div class="card mb-3"> -->
                                                        <div class="flip-card">
                                                            <div class="flip-card-inner">
                                                                <div class="card flip-card-front">
                                                                    <p class="flip-card-text">
                                                                        <%= cards[i].term %>
                                                                    </p>
                                                                </div>
                                                                <div class="card flip-card-back">
                                                                    <p class="flip-card-text">
                                                                        <%= cards[i].definition %>
                                                                    </p>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <!-- </div> -->

                                                    </div>
                                                </div>
                                                <% } %>
                                                    <a class="carousel-control-prev" href="#carouselExampleControls"
                                                        role="button" data-slide="prev">
                                                        <span class="carousel-control-prev-icon"
                                                            aria-hidden="true"></span>
                                                        <span class="sr-only">Previous</span>
                                                    </a>
                                                    <a class="carousel-control-next" href="#carouselExampleControls"
                                                        role="button" data-slide="next">
                                                        <span class="carousel-control-next-icon"
                                                            aria-hidden="true"></span>
                                                        <span class="sr-only">Next</span>
                                                    </a>
                                        </div>
                                    </div>
                                    <br>
                                    <h4 class="font-weight-bold">Terms in this set (<%= cards.length %>)</h4>


                                    <div class="card">
                                        <% for(var i=0; i < cards.length; i++){ %>
                                            <div class="card-block">
                                                <div class="row col-sm-12">
                                                    <div class="col-sm-12 col-xl-4">
                                                        <p>
                                                            <%= cards[i].term %>
                                                        </p>
                                                    </div>
                                                    <div class="col-sm-12 col-xl-7">
                                                        <p>
                                                            <%= cards[i].definition %>
                                                        </p>
                                                    </div>
                                                    <% if(owner.username===user.username){ %>
                                                        <a class="col-sm-12 col-xl-1" href="/<%= set.id %>/edit"><i
                                                                class="feather icon-edit"></i>
                                                        </a>
                                                        <% } %>
                                                </div>
                                            </div>
                                            <hr>
                                            <% } %>
                                    </div>

                                </div>

                            </div>
                        </div>
                    </div>
                </div>
                <div id="styleSelector">
                </div>
            </div>
        </div>
    </div>
    </div>
